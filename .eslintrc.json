{
    "env": {
        "browser": true,
        "es2021": true,
        "serviceworker": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
        "no-console": ["warn", { "allow": ["warn", "error", "info"] }]
    }
}
